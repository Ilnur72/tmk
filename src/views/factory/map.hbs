      <div id='map' class='maplibregl-map'>
        <div id='left' class='sidebar flex-center left collapsed'>
          <div class='sidebar-content rounded-rect flex-center'>

            <div
              id='markerList'
              style='margin:5px; padding: 5px; overflow-y: auto;'
            >
              {{#each factory}}
                <div class='intro-x'>
                  <div
                    class='box zoom-in mb-1 flex items-center px-1 py-1'
                    onclick='flyToMarker([{{this.coords}}])'
                  >
                    <div
                      class='image-fit h-10 w-10 flex-none overflow-hidden rounded-full'
                    >
                      <img
                        src='../../../public/image/marker2.png'
                        alt='Marker'
                      />
                    </div>
                    <div class='ml-4 mr-auto'>
                      <div class='font-small'>{{this.name}}</div>
                    </div>
                  </div>
                </div>
              {{/each}}
            </div>

            <div
              class='sidebar-toggle rounded-rect left'
              style='font-size: 2em'
              onclick="toggleSidebar('left')"
            >
